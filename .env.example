# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here
# OAuth Redirect URI (must match Google Cloud Console settings)
GOOGLE_REDIRECT_URI=http://localhost:3101/callback

# GTM Container ID (format: GTM-XXXXXXX)
GTM_ID=GTM-XXXXXXX

#Local directory to store GTM tokens
GTM_TOKEN_DIR=.gtm

# Optional: Service Account Key Path (for automation)
# GOOGLE_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json
