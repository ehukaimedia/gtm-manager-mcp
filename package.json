{
  "name": "gtm-manager-mcp",
  "version": "0.1.5",
  "description": "Model Context Protocol (MCP) server for managing Google Tag Manager.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gtm-mcp": "dist/bin.js",
    "gtm-mcp-callback": "dist/callbackBin.js",
    "gtm-mcp-auth": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehukaimedia/gtm-manager-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/ehukaimedia/gtm-manager-mcp/issues"
  },
  "homepage": "https://github.com/ehukaimedia/gtm-manager-mcp#readme",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node --loader ts-node/esm src/index.ts",
    "start": "node dist/index.js",
    "test": "npm run build && node tests/run-tests.mjs",
    "serve:callback": "node dist/callbackBin.js",
    "cli": "node dist/cli.js",
    "auth:url": "node dist/cli.js auth:url",
    "auth:exchange": "node dist/cli.js auth:exchange",
    "test:gtm:crud": "npm run build && node dist/test-gtm-crud.js",
    "test:gtm:submit": "npm run build && node dist/test-gtm-submit.js",
    "test:gtm:publish": "npm run build && node dist/test-gtm-publish.js",
    "list": "npm run build && node dist/list.js",
    "version:create": "node dist/cli.js version:create",
    "version:publish": "node dist/cli.js version:publish",
    "submit": "node dist/cli.js submit",
    "prepack": "npm run build",
    "prepublishOnly": "npm test"
  },
  "files": [
    "dist/index.js",
    "dist/bin.js",
    "dist/callbackBin.js",
    "dist/callbackServer.js",
    "dist/cli.js",
    "dist/list.js",
    "LICENSE",
    "README.md",
    "AGENTS.md",
    ".env.example"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-tag-manager",
    "gtm",
    "cli",
    "codex"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "google-auth-library": "^10.3.0",
    "googleapis": "^158.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
